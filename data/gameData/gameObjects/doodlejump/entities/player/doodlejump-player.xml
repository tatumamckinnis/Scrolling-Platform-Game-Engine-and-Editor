<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name  => name of the sprite
x  => sprite x pos in texture
y  => sprite y pos in texture
width  => sprite width (may be trimmed)
height  => sprite height (may be trimmed)

-->
<sprite imagePath="doodlejump-player.png" width="372" height="60">
    <frames>
    <frame name="left.png" x="0" y="0" width="62" height="60"/>
    <frame name="left_jump.png" x="62" y="0" width="62" height="60"/>
    <frame name="right.png" x="124" y="0" width="62" h="60"/>
    <frame name="right_jump.png" x="186" y="0" width="62" height="60"/>
    <frame name="shoot.png" x="248" y="0" width="62" h="60"/>
    <frame name="shoot_jump.png" x="310" y="0" width="62" height="60"/>
    </frames>
    <animations>
    </animations>
</sprite>
